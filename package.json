{
  "name": "snowball-social",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload=false --port 8080 --host 0.0.0.0",
    "prod": "concurrently \"npm run watch\" \"npm run start:server\"",
    "dev": "concurrently \"npm run start\" \"npm run dev:server\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:server": "node server.js",
    "dev:server": "nodemon -e js,json --ignore src/ server.js"
  },
  "private": true,
  "dependencies": {
