{
  "name": "snowball-social",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload=false --port 8080",
    "prod": "concurrently \"npm run watch\" \"npm run start:server\"",
    "dev": "concurrently \"npm run start\" \"npm run dev:server\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:server": "node backend/app.js",
    "dev:server": "nodemon -e js,json --ignore src/ backend/app.js",
    "e2e": "concurrently \"ng e2e\" \"npm run start:server\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
